{
  "name": "@payvost/core-banking-service",
  "version": "1.0.0",
  "description": "Core banking service for financial transactions and account management",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "prisma:generate": "prisma generate --schema=../../prisma/schema.prisma",
    "build": "npm run prisma:generate && tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "banking",
    "transactions",
    "financial",
    "core-banking"
  ],
  "author": "Payvost",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "@prisma/client": "^5.7.1",
    "prisma": "^5.7.1",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/cors": "^2.8.17",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0"
  }
}

