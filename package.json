{
  "name": "qwibik-remit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "genkit:dev": "genkit dev",
    "dev:all": "npm-run-all --parallel dev genkit:dev",
    "dev:client": "next dev --turbopack -p 3000",
    "dev:server": "cd backend && cross-env PORT=3001 ts-node-dev --prefer-ts-exts --respawn --transpile-only ./index.ts",
    "dev:pdf": "cd backend/services/pdf && npm run dev",
    "dev:email": "cd backend/services/email && npm run dev",
    "dev:admin-stats": "cd backend/services/admin-stats && npm run dev",
    "dev:webhooks": "cd backend/services/webhooks && npm run dev",
    "dev": "concurrently -k \"npm:dev:client\" \"npm:dev:server\" \"npm:dev:pdf\" \"npm:dev:email\" \"npm:dev:admin-stats\" \"npm:dev:webhooks\"",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "lint": "npx next lint",
    "typecheck": "tsc --noEmit",
    "images:optimize": "node ./scripts/optimize-images.js",
    "build:client": "next build",
    "prisma:generate": "prisma generate --schema backend/prisma/schema.prisma",
    "build:server": "npm run prisma:generate && cd backend && npm install && npm run prisma:copy && tsc -p tsconfig.json",
    "build": "npm run build:client && npm run build:server",
    "start:client": "next start -p 3000",
    "start:server": "cross-env PORT=3001 node backend/dist/index.js",
    "start": "concurrently -k \"npm:start:client\" \"npm:start:server\"",
    "db:test": "prisma db pull --schema backend/prisma/schema.prisma --print > /dev/null",
    "db:status": "prisma migrate status --schema backend/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema backend/prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema backend/prisma/schema.prisma",
    "db:push": "prisma db push --schema backend/prisma/schema.prisma",
    "db:studio": "prisma studio --schema backend/prisma/schema.prisma",
    "db:migrate-data": "tsx scripts/migrate-neon-to-prisma.ts",
    "test": "vitest run"
  },
