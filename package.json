{
  "name": "qwibik-remit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "genkit:dev": "genkit dev",
    "dev:all": "npm-run-all --parallel dev genkit:dev",
    "dev:client": "next dev -p 3000",
    "dev:server": "cross-env PORT=3001 ts-node-dev --prefer-ts-exts --respawn --transpile-only backend/index.ts",
    "dev": "concurrently -k \"npm:dev:client\" \"npm:dev:server\"",
    "genkit:watch": "genkit start -- tsx --watch src/ai/dev.ts",
    "lint": "npx next lint",
    "typecheck": "tsc --noEmit",
    "images:optimize": "node ./scripts/optimize-images.js",
    "build:client": "next build",
    "prisma:generate": "prisma generate",
    "build:server": "npm run prisma:generate && cd backend && tsc -p tsconfig.json",
    "build": "npm run build:client && npm run build:server",
    "start:client": "next start -p 3000",
    "start:server": "cross-env PORT=3001 node backend/dist/index.js",
    "start": "concurrently -k \"npm:start:client\" \"npm:start:server\"",
  "db:test": "prisma db pull --schema backend/prisma/schema.prisma --print > /dev/null",
    "db:status": "prisma migrate status --schema backend/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema backend/prisma/schema.prisma",
    "db:push": "prisma db push --schema backend/prisma/schema.prisma",
    "db:studio": "prisma studio --schema backend/prisma/schema.prisma"
    
  },

